# E2E Test Nomad Agent Configuration
# Generated from template for testing

server {
  enabled          = true
  bootstrap_expect = 1
}

client {
  enabled = true
  
  options = {
    "fingerprint.denylist" = "env_aws,env_gce,env_azure,env_digitalocean"
  }
}

ui {
  enabled = false
}

log_level = "DEBUG"

ports {
  http = {{.HTTPPort}}
  rpc  = {{.RPCPort}}
  serf = {{.SerfPort}}
}

plugin "{{.PluginName}}" {
  config {
    shell = "bash"
  }
}

plugin_dir = "{{.PluginDir}}"